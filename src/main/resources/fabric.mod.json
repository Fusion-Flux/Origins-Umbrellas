{
  "schemaVersion": 1,
  "id": "originsumbrellas",
  "version": "${version}",
  "name": "Origins: Umbrellas",
  "description": "This is a mod that adds umbrellas to Origins that protect against the elements",
  "authors": [
    "FusionFlux"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/origins-umbrellas",
    "sources": "https://github.com/Fusion-Flux/Origins-Umbrellas",
    "issues": "https://github.com/Fusion-Flux/Origins-Umbrellas/issues"
  },

  "license": "MIT",
  "icon": "assets/originsumbrellas/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.fusionflux.originsumbrellas.OriginsUmbrellas"
    ],
    "client": [
      "com.fusionflux.originsumbrellas.client.OriginsUmbrellasClient"
    ]
  },
  "mixins": [
    "originsumbrellas.mixins.json"
  ],

  "depends": {
    "minecraft": ">=${minecraft_version}",
    "fabricloader": ">=${loader_version}",
    "fabric-api": ">=${fapi_version}",
    "origins": ">=${origins_version}"
  },
  "suggests": {
    "another-mod": "*"
  }
}
